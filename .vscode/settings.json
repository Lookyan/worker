// Place your settings in this file to overwrite default and user settings.
{
    "editor.insertSpaces": true,
    "files.exclude": {
        "**/.git": true,
        "**/.DS_Store": true,
        "build": true,
        "dist": true,
        "*.egg-info": true,
        "**/*.pyc": true
    },
    "python.linting.flake8Enabled": true,
    "python.linting.flake8Path": "flake8 --max-line-length=120 --exclude=moira/graphite --ignore=E402"
}